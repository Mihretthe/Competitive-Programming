if __name__ == '__main__':
    n = int(input())
    student_marks = {}
    for _ in range(n):
        name, *line = input().split()
        scores = list(map(float, line))
        student_marks[name] = scores
    query_name = input()
    a=student_marks[query_name]
    x=0
    for b in a:x+=b
    x/=len(a)
    s="{val:.2f}"
    print(s.format(val=x))
