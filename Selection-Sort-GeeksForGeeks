void selectionSort(int arr[], int n)
    {
       int small, temp;
       for(int i=0;i<n;i++){
           small=i;
           for(int j=i+1;j<n;j++)
           {
               if(arr[j]<arr[small])
                small=j;
           }
           temp=arr[small];
           arr[small]=arr[i];
           arr[i]=temp;
           
       }    
    }
